{
  "name": "@ujeebu-org/langchain",
  "version": "0.2.1",
  "description": "LangChain integration for Ujeebu Extract API",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && sh scripts/post-build.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "langchain",
    "ujeebu",
    "extract",
    "article",
    "scraping",
    "nlp",
    "llm",
    "ai",
    "web-scraping",
    "content-extraction"
  ],
  "author": "Ujeebu <support@ujeebu.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ujeebu/ujeebu-langchain-js.git"
  },
  "bugs": {
    "url": "https://github.com/ujeebu/ujeebu-langchain-js/issues"
  },
  "homepage": "https://ujeebu.com",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@langchain/community": "^1.1.19",
    "@langchain/core": "^1.1.28",
    "@langchain/langgraph": "^1.1.5",
    "@langchain/openai": "^1.2.10",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "langchain": "^1.2.27",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.3.0 <2.0.0"
  }
}
